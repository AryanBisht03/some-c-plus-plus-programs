#include<bits/stdc++.h>
using namespace std;
int add(int a,int b)
{
	return(a+b);
}
int add(int a,int b,int c)
{
	return(a+b+c);
}
int add(int a,int b,int c,int d)
{
	return(a+b+c+d);
}
float add(float a,float b,float c,float d)
{
	return (a+b+c+d);
}

float add(float a,float b)
{
	return (a+b);
}

int add(float a,float b,float c)
{
	return (a+b+c);
}
int main()
{
	//function overloading
	cout<<add(2,3)<<endl;
	cout<<add(2,3,4)<<endl;
	cout<<add(12,23,34,45)<<endl;
    cout<<add(12.23f,23.34f)<<endl;
    cout<<add(1.2f,3.4f,5.6f,6.7f)<<endl;
	cout<<add(12.23f,34.56f,45.33)<<endl;
	return 0;
}
